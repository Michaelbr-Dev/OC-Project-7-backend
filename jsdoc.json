{
  "tags": {
    "dictionaries": ["jsdoc"],
    "allowUnknownTags": ["category"]
  },
  "source": {
    "include": ["./", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "node_modules"
  },
  "plugins": ["plugins/markdown", "node_modules/better-docs/category"],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/better-docs",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "search": true,
    "better-docs": {
      "name": "Groupomania API documentation",
      "title": "Groupomania API documentation",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/Michaelbr-Dev/OC-Project-7-backend"
        }
      ]
    }
  }
}